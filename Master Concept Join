use demo_database;

DROP table TABLE2;

CREATE TABLE  TABLE1(
NAME1 int
);

INSERT INTO TABLE1
VALUES
(1),(1),(NULL),(2),(3),(NULL),(4);

SELECT * FROM TABLE1;
select * from TABLE2;

SELECT COUNT(*) FROM TABLE1;

CREATE TABLE  TABLE2(
NAME2 int
);

INSERT INTO TABLE2
VALUES
(1),(2),(2),(null),(NULL),(3),(3),(4);

SELECT * FROM TABLE2;
SELECT * FROM TABLE1;
SELECT COUNT(*) FROM TABLE2;


--inner join
SELECT DISTINCT * FROM TABLE1 T1 
INNER JOIN TABLE2 T2 ON T1.NAME1=T2.NAME2
order by t1.name1;

--left outer join
SELECT distinct name1 FROM TABLE1 T1 
left outer JOIN TABLE2 T2 ON T1.NAME1=T2.NAME2
order by t1.name1;


--right outer join
SELECT DISTINCT * FROM TABLE1 T1 
right outer JOIN TABLE2 T2 ON T1.NAME1=T2.NAME2
order by t1.name1;


--full outer join
SELECT DISTINCT * FROM TABLE1 T1 
full outer JOIN TABLE2 T2 ON T1.NAME1=T2.NAME2
order by t1.name1;

SELECT * FROM TABLE1
union
SELECT * FROM TABLE2;


--cross join
SELECT distinct * FROM TABLE1
cross JOIN TABLE2;


--self join 
SELECT distinct * FROM TABLE1 as T1 
join TABLE1 as T2 ON T1.NAME1=T2.NAME1;

SELECT distinct * FROM TABLE2 as T1
join TABLE2 as T2 ON T1.NAME2=T2.NAME2;
